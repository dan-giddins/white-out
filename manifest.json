{
  "manifest_version": 3,
  "name": "WhiteOut",
  "version": "1.0",
  "description": "Force a forground text colour of black and a background colour of white.",

  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "css": [
        "styles.css"
      ],
      "matches": [
        "https://*/*"
      ]
    }
  ]
}
